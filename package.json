{
  "name": "book-trading-app",
  "version": "1.0.0",
  "description": "an application that allows users to login and trade books",
  "main": "server",
  "scripts": {
    "remove:build": "rimraf dist",
    "dev": "npm run remove:build && nodemon",
    "prod:build:server-renderer": "cross-env NODE_ENV=production webpack --config config/webpack/serverRenderer",
    "prod:build:client": "cross-env NODE_ENV=production webpack --config config/webpack/client",
    "prod:build": "npm run remove:build && npm-run-all --parallel prod:build:*",
    "prod": "npm run prod:build && npm run start",
    "start": "node server",
    "test:client": "cross-env NODE_ENV=testing jest --config test/client/jest.config.js --watch test/client --runInBand",
    "test:api:models": "cross-env NODE_ENV=testing jest --config test/api/jest.config.js --watch test/api/models --runInBand",
    "test:api:controllers": "cross-env NODE_ENV=testing jest --config test/api/jest.config.js --watch test/api/controllers --runInBand",
    "test:api:routes": "cross-env NODE_ENV=testing jest --config test/api/jest.config.js --watch test/api/routes --runInBand",
    "test:api:services": "cross-env NODE_ENV=testing jest --config test/api/jest.config.js --watch test/api/services"
  },
  "author": "steve barton",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "gsap": "^2.0.2",
    "helmet": "^3.13.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^5.2.10",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^4.0.0-beta.4",
    "styled-icons": "^2.4.0",
    "webpack": "^4.21.0",
    "webpack-flush-chunks": "^2.0.1",
    "winston": "^3.0.0",
    "xmljson": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-styled-components-postcss": "^1.0.5-alpha.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-universal-import": "^3.0.2",
    "compression-webpack-plugin": "^1.1.11",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "history": "^4.7.2",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "moxios": "^0.4.0",
    "postcss": "^7.0.5",
    "react-hot-loader": "^4.3.5",
    "react-testing-library": "^4.1.9",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.9",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  }
}
